{
    "debug": true,
    "domain": "sdslabs.co",
    "secret": "YOUR_SECRET_KEY",
    "projectRoot": "/SWS",
    "rcFile": "Gasperfile",
    "offlineMode": false,
    "cron": {
        "cleanupInterval": 600,
        "exposureInterval": 30
    },
    "mongo": {
        "url": "mongodb://localhost:27017"
    },
    "redis": {
        "host": "localhost",
        "port": ":6379",
        "password": "",
        "DB": 0
    },
    "falcon": {
        "plugIn": false,
        "falconClientId": "",
        "falconClientSecret": "",
        "falconUrlAccessToken": "http://falcon.sdslabs.local/access_token",
        "falconUrlResourceOwnerDetails": "http://falcon.sdslabs.local/users/",
        "falconAccountsUrl": "http://arceus.sdslabs.local/",
        "redirectUri": ""
    },
    "services": {
        "dominus": {
            "deploy": true,
            "port": ":3000"
        },
        "ssh": {
            "deploy": true,
            "port": ":2222",
            "host_signers": ["/home/user/.ssh/id_rsa"],
            "using_passphrase": false,
            "passphrase": ""
        },
        "ssh_proxy": {
            "deploy": true,
            "port": ":3222",
            "host_signers": ["/home/user/.ssh/id_rsa"],
            "using_passphrase": false,
            "passphrase": ""
        },
        "static": {
            "deploy": true,
            "image": "nginx:latest",
            "port": ":8000"
        },
        "php": {
            "deploy": true,
            "image": "nginx:latest",
            "port": ":8001"
        },
        "node": {
            "deploy": true,
            "image": "nginx:latest",
            "port": ":8002"
        },
        "python": {
            "deploy": true,
            "python2_image": "6b9228af2a0a",
            "python3_image": "nginx:latest",
            "port": ":8003"
        },
        "mysql": {
            "deploy": true,
            "image": "mysql:5.7",
            "port": ":9000",
            "container_port": "33060",
            "env": {
                "MYSQL_USER": "root",
                "MYSQL_ALLOW_EMPTY_PASSWORD": "yes"
            }
        },
        "mongodb": {
            "deploy": true,
            "image": "mongo:latest",
            "port": ":9001",
            "container_port": "27018",
            "env": {
                "MONGO_INITDB_ROOT_USERNAME": "",
                "MONGO_INITDB_ROOT_PASSWORD": ""
            }
        }
    }
}
